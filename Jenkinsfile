pipeline {
    agent any
    stages {
        stage('Compile') {
            steps {
                sh 'echo Compiling the project'
            }
        }
        stage('Test') {
            steps {
                sh 'echo Running tests'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo Deploying application'
            }
        }
    }
}
